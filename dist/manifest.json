{
  "manifest_version": 3,
  "name": "DARK-MODE-EXTENSION",
  "version": "1.0",
  "description": "A dark mode theme extension using Tailwind and TypeScript.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "128": "icons/icon128.png"
  }
}
